.tt-cursor { color: white; background: linear-gradient(rgb(56, 117, 215), rgb(42, 98, 188)); }

.tt-menu { background: linear-gradient(rgb(49, 49, 49), rgb(19, 19, 19)); color: white; width: calc(100% + 73px); max-height: 240px; overflow: auto; left: -36px !important; }

.tt-menu::-webkit-scrollbar { width: 0.5em; height: 0.5em; }

.tt-menu::-webkit-scrollbar-thumb { background: rgb(102, 102, 102); }

.tt-menu::-webkit-scrollbar-track { background: none; }

.empty-typeahead-message { padding: 15px; color: white; }

.typeahead-no-results-found { text-align: center; padding: 1rem; }

.twitter-typeahead { -webkit-box-flex: 1; flex: 1 1 0%; }

@media print {
  .main-sidebar, [id="react-search-and-user-menu"], .width-wrapper-comments .explore-tabs, .link-footer, .end-of-article-footer, .article-headline-link, .blog-meta-spacer, .blog-meta-spacer + .author-link, .actions-meta a, .cp_embed_wrapper iframe, [id="react-site-footer"], .end-of-article-sep, .follow-user-button, .pick-iframe, .leave-a-comment-area, .block-comment-actions { display: none; }
  body { overflow: visible; padding: 1rem !important; }
  body, .block-comment-content, pre { background: white !important; color: black !important; }
  .box { border: 0.5em double rgb(102, 102, 102); margin: 0px 0px 1em; }
  .post-body pre[class]::before, .comment-text pre[class]::before { border-bottom: 0.3em solid rgb(102, 102, 102); }
  .post-body pre[class*="lang-"], .comment-text pre[class*="lang-"] { margin: 0px; }
  .cp_embed_wrapper { border: 0.1em dashed rgb(102, 102, 102); height: 4em; margin: 0px 0px 1em; overflow: hidden; padding: 1em; }
  .cp_embed_wrapper::before { content: "Embedded Pen"; display: block; margin: 0px 0px 4em; }
  .mega-header { box-shadow: none; background: none; border: 0px; }
  .comment-list { list-style: none; }
